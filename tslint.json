{
    "rulesDirectory": [
        "./resources/assets/js"
    ],
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "interface-name": false,
        "no-console": [false],
        "no-implicit-dependencies": false,
        "no-submodule-imports": [true, "config", "store", "components", "pages", "utils", "services", "types"]
    }
}
